{
  "expo": {
    "name": "Recytech",
    "slug": "Recytech-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon-2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/splash-icon-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
"android": {
  "useNextNotificationsApi": true,
  "permissions": [
    "READ_EXTERNAL_STORAGE",
    "WRITE_EXTERNAL_STORAGE",
    "MANAGE_EXTERNAL_STORAGE",
    "NOTIFICATIONS"
  ],
  "googleServicesFile": "./google-services.json",
  "adaptiveIcon": {
    "foregroundImage": "./assets/icons/app-icon-2.png",
    "backgroundColor": "#ffffff"
  },
  "package": "com.rlatap.RecytechApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "ios": {
            "bundleIdentifier": "com.rlatap.RecytechApp"
          },
          "android": {
            "package": "com.rlatap.RecytechApp"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/notification-icon.png",
          "color": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3224daf7-e979-41b9-9f07-ba594311ce17"
      }
    }
  }
}
